import java.util.Arrays;

public class Ex01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr= new int[] {1,2,3,4,5,6,7,8,9};
		
		int[] brr = new int[10];//khai bao 1 mang voi do dai =10
		System.out.println(brr[0]);//mang thi default value la 0
		System.out.println("The length of array brr is: " + brr.length);
		
		String[] crr = new String[10];// string default value la null
		int count = 0;
		System.out.println("crr[1] la " + crr[1]);
		
		crr[count++]= "Thanh TUng";
		crr[count++]= "Thanh TUng";
		crr[count++]= "Thanh TUng";
		crr[count++]= "Thanh TUng";
		crr[count++]= "Thanh TUng";
		System.out.println(Arrays.toString(crr));//su dung thu vien array de in mang hoac chay vong lap
	}

}
