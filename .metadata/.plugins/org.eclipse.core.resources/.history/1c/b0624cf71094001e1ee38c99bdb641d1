
public class Node <T> {
	int data; //luu gia tri 1 phan tu trong danh sach
	Node next;//luu dia chi cua phan tu sau
	
	public Node(int data) {
		this.data = data;
		this.next = null;
	}
	
	public static void main(String[] args) {
		Node one = new Node(9);
		Node two = new Node(7);
		Node three = new Node(12);
		Node four = new Node(5);
	}
}
